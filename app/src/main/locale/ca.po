msgid "pref_key_notif_access_perm"
msgstr ""

msgctxt "notif_access_summary"
msgid "For Alamist to work, you must click here and allow Notification access"
msgstr ""

msgctxt "notif_access_title"
msgid "Install Alarmist"
msgstr ""

