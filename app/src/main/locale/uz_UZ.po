msgstr "<xliff:g id=\"NUM_TIMERS\">%d</xliff:g> ta taymer vaqti tugadi"

#. Label associated with a notification for a Timer
msgctxt "timer_notification_label"
msgid "Timer"
msgstr "Taymer"

